#!/bin/sh

# Starta VPN (BSD/OSX)
vpn()
{
  if [ "$1" == "start" ]; then
    echo "Connecting to $VPN"
      scutil --nc start "$VPN" 
  else
    echo "Disconnecting from $VPN"
      scutil --nc stop "$VPN"
      fi
}

mail()
{
  ICLOUD_LOGIN="$(sudo security find-generic-password -s icloud-mutt -w)"
  mutt "$@"
}
